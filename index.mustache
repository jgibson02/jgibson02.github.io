<body>
	<section id="leftWrapper">
		{{#about-me}}
		<section id="AboutMe">
			<div class="card-wrapper">
				<img src="profile_300px.png" class="avatar"></img>
				<h1 id="name">{{about-me.first-name}} {{about-me.last-name}}</h1>
				<h5 class="jobtitle">{{about-me.position}}</h5>
			</div>
		</section>
		<section class="contact-details card-wrapper">
			<div class="detail">
				<span class="detail-icon">
					<i class="detail-icon icon-location"></i>
				</span>
				<span class="detail-info">
					{{about-me.location}}
				</span>
			</div>

			<div class="detail">
				<span class="detail-icon">
					<i class="detail-icon icon-phone"></i>
				</span>
				<span itemprop="telephone" class="detail-info">
					<a href="tel:+{{about-me.mobile}}" alt="My Cell Phone Number" class="link-disguise ">{{about-me.mobile-display}}</a>
				</span>
			</div>

			<div class="detail">
				<span class="detail-icon">
					<i class="detail-icon icon-mail"></i>
				</span>
				<span class="detail-info">
					<a href="mailto:{{about-me.email}}" alt="My Email Address" class="link-disguise" rel="noopener">{{about-me.email}}</a>
				</span>
			</div>

			<div class="detail">
				<span class="detail-icon">
					<i class="detail-icon icon-link"></i>
				</span>
				<span class="detail-info">
					<a href="{{about-me.website}}" target="_blank" rel="noopener">{{about-me.website}}</a>
				</span>
			</div>

			<div class="detail">
				<span class="detail-icon">
					<i class="detail-icon icon-file-pdf"></i>
				</span>
				<span class="detail-info">
					<a href="https://drive.google.com/file/d/0B7qOx74qe3aJOV9MSXQwLXY3cnc/view?usp=sharing" target="_blank" class="link-disguise" rel="noopener">Résumé</a>
				</span>
			</div>
			<hr>
			<div class="social-links text-center">
				<div class="social-links-text-center">
					<a href="https://github.com/{{about-me.github}}" target="_blank" rel="noopener" title="John Gibson on Github" class="social-link link-github"><i class="fab fa-github"></i></a>
					<a href="https://codepen.io/{{about-me.codepen}}" target="_blank" rel="noopener" title="John Gibson on Codepen" class="social-link link-codepen"><i class="fab fa-codepen"></i></a>
					<a href="https://www.linkedin.com/in/{{about-me.linkedin}}" target="_blank" rel="noopener" title="John Gibson on Linkedin" class="social-link link-linkedin"><i class="fab fa-linkedin"></i></a>
					<a href="https://www.medium.com/@{{about-me.medium}}" target="_blank" rel="noopener" title="John Gibson on Medium" class="social-link link-medium"><i class="fab fa-medium"></i></a>
				</div>
			</div>
		</section>

		<div>
			<a href="https://github.com/{{about-me.github}}/Fronds/releases/download/v1.0/Fronds.exe" rel="noopener">
				<img src="schwimmer.png" height=1 width=1/>
			</a>
		</div>
	</section>
	{{/about-me}}
	<div id="rightWrapper" class="card-wrapper">
		<ul class="nav nav-tabs">
			<li class="active"><a data-toggle="tab" href="#background">Background</a></li>
			<li><a data-toggle="tab" href="#calendarTab">Calendar</a></li>
			<li><a data-toggle="tab" href="#portfolio">Codepens</a></li>
		</ul>
		<div class="tab-content">
			<div id="background" class="tab-pane fade in active">
				<h4 class="text-uppercase">Background</h4>
				<hr>
				<div id="background-details">
					<div id="about" class="detail">
						<div class="icon">
							<i class="fs-lg icon-profile"></i>
						</div>
						<div class="info">
							<h4 class="title text-uppercase">About</h4>
							<div itemprop="description" class="content">
								{{about-me.description}}
							</div>
						</div>
					</div>
					<div id="work-experience" class="detail">
						<div class="icon">
							<i class="fs-lg icon-work"></i>
						</div>
						<div class="info">
							<h4 class="title text-uppercase">Work Experience</h4>
							<ul class="list-unstyled">
								{{#experience}}
								<li class="card card-nested clearfix">
									<div class="content">
										<p class="clear-margin relative">
											{{#active}}<i rel="tooltip" title="Currently Working" class="current-event"></i>{{/active}}
											<strong>{{title}}, <a href="{{website}}">{{institution}}</a></strong>
										</p>
										<p class="text-muted"><small>
											<span class="mr-10">{{date-range}}</span>
											<span><i class="icon-clock mr-5"></i>{{span}}</span>
										</small></p>
										<ul>
											{{#list-items}}
												<li>{{.}}</li>
											{{/list-items}}
										</ul>
									</div>
								</li>
								{{/experience}}
							</ul>
						</div>
					</div>
					<div id="education" class="detail">
						<div class="icon">
							<i class="fs-lg icon-graduation-cap"></i>
						</div>
						<div class="info">
							<h4 class="title text-uppercase">Education</h4>
							<ul class="list-unstyled">
								{{#education}}
								<li class="card card-nested clearfix">
									<div class="content">
										<p class="clear-margin relative">
											{{#active}}<i rel="tooltip" title="Currently Pursuing" data-placement="left" class="current-event"></i>{{/active}}
											<strong>{{study}}, {{institution}}</strong>
										</p>
										<p class="text-muted"><small>{{date-range}}<span style="margin:0 0.5em">|</span><strong>Cum. GPA: {{gpa}}</strong></small></p>
										<ul>
											{{#list-items}}<li>{{.}}</li>{{/list-items}}
										</ul>
									</div>
								</li>
								{{/education}}
							</ul>
						</div>
					</div>
					<div id="activities" class="detail">
						<div class="icon">
							<i class="fs-lg icon-heart"></i>
						</div>
						<div class="info">
							<h4 class="title text-uppercase">Activities</h4>
							<ul class="list-unstyled">
								{{#extracurricular}}
								<li class="card card-nested">
									<div class="content">
										<p class="clear-margin relative">
											{{#active}}<i rel="tooltip" title="Currently Participating" class="current-event"></i>{{/active}}
											<strong>{{role}}</strong>
										</p>
										<p class="text-muted"><small>{{date-range}}</small></p>
										<ul>
											{{#list-items}}
												<li>{{.}}</li>
											{{/list-items}}
										</ul>
									</div>
								</li>
								{{/extracurricular}}
							</ul>
						</div>
					</div>
					<div class="detail skills">
						<div class="icon">
							<i class="fs-lg icon-hammer"></i>
						</div>
						<div class="info">
							<h4 class="title text-uppercase">Skills</h4>
							<ul class="list-unstyled">
								<li class="card card-nested">
									<div class="content">
										<table>
											{{#skills}}
								            <tr>
								                <td class="skills__heading">{{heading}}</td>
								                <td class="skills__list">
								                	{{#list-items}}
								                		<span>{{.}} </span> 
								                	{{/list-items}}
								                </td>
								            </tr>
								            {{/skills}}
							        </table>
									</div>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
			<div id="calendarTab" class="tab-pane fade">
				<div id="calendar"></div>
			</div>
